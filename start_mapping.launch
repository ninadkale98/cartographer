<launch>

    <node name="cartographer_node" pkg="cartographer_ros"
        type="cartographer_node" args="
            -configuration_directory $(find cartographer_ros)/configuration_files
            -configuration_basename jet2.lua"
        output="screen">
    <remap from="scan" to="/scan" />
    <remap from="odom" to="/camera/odom/sample" />
    <remap from="imu" to="/imu" />
    </node>

    <node name="cartographer_occupancy_grid_node" pkg="cartographer_ros"
        type="cartographer_occupancy_grid_node" args="-resolution 0.025" />

<node pkg="tf" type="static_transform_publisher" name="camera_to_baselink" args="0 0 0 0 0 0 camera_pose_frame base_link 100" />
<node pkg="tf" type="static_transform_publisher" name="camera_odom_tomap" args="0 0 0 0 0 0 map camera_odom_frame 100" />

</launch>